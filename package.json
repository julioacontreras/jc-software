{
  "name": "jc",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test": "npm run test:e2e && npm run test:unit",
    "build:e2e": "vite build --mode development",
    "preview:e2e": "vite preview --mode development",
    "test:e2e": "playwright test",
    "test:unit": "vitest",
    "deploy": "vite build && node ./fixApp.js && npx gh-pages -d public -t true && rm -rf public"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.20.4",
    "@testing-library/svelte": "^4.0.4",
    "@tsconfig/svelte": "^5.0.2",
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.68.0",
    "svelte": "^4.0.5",
    "svelte-adapter-bun": "^0.5.0",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.4.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.2",
    "vitest": "^0.32.2"
  },
  "type": "module",
  "dependencies": {
    "@rollup/plugin-typescript": "^11.1.4",
    "svelte-use-form": "^2.10.0"
  }
}
